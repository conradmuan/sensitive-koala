{
  "name": "sensitive-content-bot",
  "version": "1.0.0",
  "description": "Slack app to add sensitive content",
  "main": "dist/index.js",
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "tsc && node dist/index",
    "delay": "1000",
    "ext": "ts"
  },
  "scripts": {
    "start": "node dist/index",
    "build": "tsc",
    "postinstall": "npm run build",
    "dev": "nodemon"
  },
  "author": "Conrad Muan <hi@conrad.chat>",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/sequelize": "^4.28.13",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "nodemon": "^2.0.16",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "qs": "^6.10.3",
    "sequelize": "^6.19.0",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.32",
    "tslib": "^2.4.0"
  }
}
